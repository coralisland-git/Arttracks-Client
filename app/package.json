{
  "private": true,
  "name": "arttracks",
  "version": "4.0.0",
  "description": "Arttracks.com Public Open Beta",
  "homepage": "https://www.arttracks.com/",
  "watch": {
    "dev:server": "tmp/bundle.js"
  },
  "engines": {
    "node": "8.8.1"
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:js": "rimraf public/js/main*.js && rimraf public/js/app.js && rimraf public/js/plugins.js && rimraf public/js/*.map",
    "clean:css": "rimraf public/css/main*.css && rimraf public/css/app*.css && rimraf public/css/*.map",
    "clean:fonts": "rimraf public/fonts/*",
    "clean:server": "rimraf server.js && rimraf server.js.map",
    "clean:tmp": "rimraf tmp",
    "clean": "run-s clean:dist clean:tmp clean:server clean:css clean:js clean:fonts",
    "update:schema": "babel-node tools/update-schema.js",
    "load-sample": "babel-node tools/load-sample-data.js",
    "hot:client": "cross-env NODE_ENV=development EXTRACT_TEXT_PLUGIN=false webpack-dev-server --host 0.0.0.0 --config tools/webpack.client.js",
    "hot:server": "cross-env NODE_ENV=development webpack --colors --display-error-details --watch --verbose --config tools/webpack.server.js",
    "dev:server": "cross-env NODE_ENV=development node tmp/bundle.js",
    "dev:server:wait": "node node_modules/just-wait -p \"tmp/bundle.js\" --timeout 120 && npm run start-watcher",
    "start-watcher": "npm-watch",
    "watch-build-server": "run-s clean:tmp hot:server",
    "dev": "concurrently --kill-others \"npm run watch-build-server\" \"npm run hot:client\" \"npm run dev:server:wait\"",
    "dev:rtl": "cross-env RTL=true run-s dev",
    "dev:dist": "run-p dev dist:watch",
    "build:server": "cross-env NO_OUTPUT_PATH=true NODE_ENV=production webpack --progress --colors --display-error-details --config tools/webpack.server.js",
    "build:client": "cross-env EXTRACT_TEXT_PLUGIN=true webpack --progress --colors --display-error-details --config tools/webpack.client.js",
    "bundle:server": "run-s clean:tmp clean:server build:server",
    "bundle:client": "run-s clean:css clean:js clean:fonts build:client",
    "prod:server": "cross-env NODE_ENV=production node server.js",
    "build:prod": "cross-env NODE_ENV=production run-s bundle:client bundle:server",
    "build:prod:rtl": "cross-env NODE_ENV=production RTL=true run-s bundle:client bundle:server",
    "prod": "run-s build:prod prod:server",
    "prod:rtl": "run-s build:prod:rtl prod:server",
    "dist": "cross-env NODE_ENV=production run-s bundle:client && node tools/create-dist.js",
    "dist:watch": "run-s bundle:client && node tools/watch-dist.js",
    "test": "mocha --compilers :./tools/babel-setup.js --recursive --watch ./test/*.js",
    "start": "node server.js"
  },
  "author": "Sketchpixy <support@sketchpixy.com>",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "bless-webpack-plugin": "^1.0.0",
    "chokidar": "^1.5.2",
    "concurrently": "^2.1.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "deepmerge": "^0.2.10",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isomorphic-style-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "just-wait": "^1.0.5",
    "loader-utils": "^0.2.15",
    "mocha": "^10.2.0",
    "ncp": "^2.0.0",
    "node-sass": "^3.13.1",
    "npm-run-all": "^2.2.0",
    "npm-watch": "^0.1.4",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "rtlcss": "^2.0.5",
    "sass-loader": "^3.2.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "uglify-loader": "^1.3.0",
    "url-loader": "^0.5.7",
    "video.js": "^5.14.1"
  },
  "dependencies": {
    "@sketchpixy/rubix": "^2.9.16",
    "axios": "^0.13.1",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin-loader": "^0.9.0",
    "body-parser": "^1.20.1",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "classnames": "^2.2.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.3",
    "country-list": "^1.1.0",
    "downloadjs": "^1.4.6",
    "easy-countries-list": "^1.1.0",
    "es6-promise": "^3.2.1",
    "express": "^4.18.2",
    "fabric": "^1.7.19",
    "filepicker-js": "^2.4.17",
    "filesize": "^3.3.0",
    "filestack-js": "^0.11.4",
    "fs": "0.0.1-security",
    "heroku-ssl-redirect": "0.0.4",
    "jsdom": "^9.12.0",
    "moment": "^2.17.0",
    "node-gyp": "^3.6.2",
    "node-stopwords-filter": "0.0.1",
    "object-assign": "^4.1.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-soundcloud": "^0.2.0",
    "passport-youtube-v3": "^2.0.0",
    "pug": "^2.0.0-beta3",
    "pusher-js": "^4.0.0",
    "querystring": "^0.2.0",
    "ramda": "^0.23.0",
    "rc-slider": "^5.4.3",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.1",
    "react-cookie": "^0.4.8",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.9.2",
    "react-facebook-login-component": "^0.6.0",
    "react-ga": "^2.2.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-intercom": "^1.0.11",
    "react-log-lifecycle": "^1.0.0",
    "react-pusher": "^0.1.1",
    "react-router": "^3.0.0-beta.1",
    "react-timeout": "^1.0.0",
    "react-toggle": "^2.1.1",
    "request": "^2.86.0",
    "sanitize-filename": "^1.6.1",
    "simple-html-tokenizer": "^0.2.5",
    "source-map-support": "^0.4.0",
    "sprintf-js": "^1.1.1",
    "stopword": "^0.1.6",
    "stripe": "^6.1.1",
    "svg-inline-loader": "^0.7.1",
    "svg-inline-react": "^1.0.2",
    "uuid-v4": "^0.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
